{
    "presets": [
        "@babel/preset-typescript",
        "@babel/preset-env"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/plugin-transform-typescript",
        ["@babel/plugin-proposal-pipeline-operator", {"proposal": "fsharp"}]
    ],
    "parserOpts": {
        "plugins": ["typescript"]
    }
}